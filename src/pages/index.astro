<style>
main {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	gap: 3.2rem;
	height: 100vh;
	/* min-height: 72rem; */
	margin: auto;
	padding: 1rem;


	background-color: hsl(329.21, 79.17%, 18.82%);
	background-image: url("/images/jr-korpa-9XngoIpxcEo-unsplash-60-800.jpg");
	background-position: center center;
	background-repeat: no-repeat;
	background-size: cover;
}

	main .logo svg {
		width: 200px;
	}

	main h1 {
		margin-top: 0;
		margin-bottom: 0;

		font-family: var(--heading-font);
		font-size: clamp(7rem, 20vw, 24rem);
		font-weight: 900;
  	letter-spacing: .02rem;
  	line-height: 1.15;
	}

	main .message {
		text-align: center;
	}

@media (min-width: 401px) {
	main {
		background-image: -webkit-image-set(
			url("/images/jr-korpa-9XngoIpxcEo-unsplash-60-800.jpg") 1x,
			url("/images/jr-korpa-9XngoIpxcEo-unsplash-60-1080.jpg") 2x
		);
		background-image: image-set(
			url("/images/jr-korpa-9XngoIpxcEo-unsplash-60-800.jpg") 1x,
			url("/images/jr-korpa-9XngoIpxcEo-unsplash-60-1080.jpg") 2x
		);
	}
}

